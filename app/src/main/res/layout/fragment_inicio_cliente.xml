<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:clickable="true"
    android:focusable="true"
    tools:context=".InicioClienteFragment">

    <View
        android:id="@+id/view2"
        android:layout_width="413dp"
        android:layout_height="514dp"
        android:background="#FFFFFF"
        android:backgroundTint="#BF5A7F"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view3"
        android:layout_width="411dp"
        android:layout_height="218dp"
        android:foreground="@drawable/ic_wave"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" tools:targetApi="m"/>

    <Button
        android:id="@+id/btn_cat"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:alpha="1"
        android:background="@drawable/redondez_botones"
        android:backgroundTintMode="multiply"
        android:drawableTop="@drawable/ic_baseline_cake_24"
        android:elevation="100sp"
        android:gravity="center"
        android:outlineProvider="none"
        android:singleLine="true"
        android:soundEffectsEnabled="true"
        android:text="@string/cat_logo"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_carrito"
        app:layout_constraintEnd_toStartOf="@+id/btn_coti"
        app:layout_constraintHorizontal_bias="0.561"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.622" />

    <Button
        android:id="@+id/btn_coti"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:alpha="1"
        android:background="@drawable/redondez_botones"
        android:backgroundTintMode="multiply"
        android:drawableTop="@drawable/ic_baseline_create_24"
        android:elevation="100sp"
        android:gravity="center"
        android:outlineProvider="none"
        android:singleLine="true"
        android:soundEffectsEnabled="true"
        android:text="@string/cotizaci_n"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_pedido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.854"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.622" />

    <Button
        android:id="@+id/btn_carrito"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginBottom="52dp"
        android:alpha="1"
        android:background="@drawable/redondez_botones"
        android:backgroundTintMode="multiply"
        android:drawableTop="@drawable/ic_baseline_shopping_cart_24"
        android:elevation="100sp"
        android:gravity="center"
        android:outlineProvider="none"
        android:singleLine="true"
        android:soundEffectsEnabled="true"
        android:text="@string/carrito"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_ofertas"
        app:layout_constraintEnd_toStartOf="@+id/btn_pedido"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_pedido"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:alpha="1"
        android:background="@drawable/redondez_botones"
        android:backgroundTintMode="multiply"
        android:drawableTop="@drawable/ic_baseline_assignment_24"
        android:elevation="100sp"
        android:gravity="center"
        android:outlineProvider="none"
        android:singleLine="true"
        android:soundEffectsEnabled="true"
        android:text="@string/pedido"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_usuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.854"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.83" />

    <Button
        android:id="@+id/btn_ofertas"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:alpha="1"
        android:background="@drawable/redondez_botones"
        android:backgroundTintMode="multiply"
        android:drawableTop="@drawable/ic_baseline_local_offer_24"
        android:elevation="100sp"
        android:gravity="center"
        android:outlineProvider="none"
        android:singleLine="true"
        android:soundEffectsEnabled="true"
        android:text="@string/ofertas"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:layout_constraintVertical_bias="0.774" />

    <Button
        android:id="@+id/btn_usuario"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:alpha="1"
        android:background="@drawable/redondez_botones"
        android:backgroundTintMode="multiply"
        android:drawableTop="@drawable/ic_baseline_user"
        android:elevation="100sp"
        android:gravity="center"
        android:outlineProvider="none"
        android:singleLine="true"
        android:soundEffectsEnabled="true"
        android:text="@string/usuario"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.854"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.773" />

    <ImageButton
        android:id="@+id/opciones"
        style="@android:style/Widget.Holo.Light.ImageButton"
        android:layout_width="58dp"
        android:layout_height="43dp"
        android:background="@drawable/ic_baseline_menu_24"
        android:contentDescription="@string/todo"
        android:tint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/btn_cat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.388" />

</androidx.constraintlayout.widget.ConstraintLayout>